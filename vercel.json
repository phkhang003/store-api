{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/swagger/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/api/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}