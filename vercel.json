{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/api/swagger-ui-bundle.js",
        "dest": "https://cdn.jsdelivr.net/npm/swagger-ui-dist@5.9.0/swagger-ui-bundle.js",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/api/swagger-ui-standalone-preset.js",
        "dest": "https://cdn.jsdelivr.net/npm/swagger-ui-dist@5.9.0/swagger-ui-standalone-preset.js",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/api/swagger-ui.css",
        "dest": "https://cdn.jsdelivr.net/npm/swagger-ui-dist@5.9.0/swagger-ui.css",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/api" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept, Authorization" }
        ]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}