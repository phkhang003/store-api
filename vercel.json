{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/swagger/(.*)",
        "dest": "/src/main.ts"
      },
      {
        "src": "/api/(.*)",
        "dest": "/src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}