{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/swagger/(.*)",
        "dest": "/src/main.ts",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
        }
      },
      {
        "src": "/api/(.*)",
        "dest": "/src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
        }
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}